@model MovieRent.ViewModels.MovieViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>

<ul style="list-style-type:none">
    @foreach (var movie in Model.movieList)
    {
        <li style="font-size:25px">@movie.name</li>
    }
</ul>

